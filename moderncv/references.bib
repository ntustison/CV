	doi = {10.1016/j.neurobiolaging.2018.05.023},
	journal = {Neurobiol Aging},
	journal-full = {Neurobiology of aging},
	keywords = {APOE; Alzheimer's disease; High-resolution fMRI; Hippocampus; Pattern separation},
	mesh = {African Americans; Aged; Alzheimer Disease; Apolipoprotein E4; Brain Mapping; Case-Control Studies; Discrimination (Psychology); Female; Genetic Predisposition to Disease; Hippocampus; Humans; Magnetic Resonance Imaging; Male; Middle Aged},
	month = {09},
	pages = {221-229},
	pmc = {PMC6392014},
	pmid = {29909179},
