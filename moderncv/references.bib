	doi = {10.1088/0031-9155/59/13/3599},
	journal = {Phys Med Biol},
	journal-full = {Physics in medicine and biology},
	mesh = {Brain; Humans; Magnetic Resonance Imaging; Phantoms, Imaging; Skull; Tomography, X-Ray Computed; Ultrasonic Therapy},
	month = {Jul},
