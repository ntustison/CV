	doi = {10.1038/s41598-021-87564-6},
	journal = {Sci Rep},
	journal-full = {Scientific reports},
	mesh = {Adult; Aged; Algorithms; Brain; Ecosystem; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Male; Middle Aged; Neuroimaging; Software},
	month = {Apr},
	number = {1},
