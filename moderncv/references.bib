	mesh = {Adult; Aged; Aged, 80 and over; Alzheimer Disease; Brain; Computational Biology; Databases, Factual; Female; Genome-Wide Association Study; Humans; Magnetic Resonance Imaging; Male; Meta-Analysis as Topic; Middle Aged; Neuroimaging; Polymorphism, Single Nucleotide; Young Adult},
	number = {5},
