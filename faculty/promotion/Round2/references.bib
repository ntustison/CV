	mesh = {Algorithms; Brain; Data Display; Foot; Humans; Image Enhancement; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Multiple Sclerosis},
	month = {Dec},
	number = {6},
	pages = {1072-81},
	pmid = {10571928},
