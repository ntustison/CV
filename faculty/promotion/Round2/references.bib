	mesh = {Adolescent; Adult; Aged; Aged, 80 and over; Aging; CA3 Region, Hippocampal; Dentate Gyrus; Discrimination, Psychological; Entorhinal Cortex; Female; Humans; Male; Nerve Net; Photic Stimulation; Young Adult},
	month = {03},
	number = {5},
