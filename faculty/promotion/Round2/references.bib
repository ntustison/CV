	doi = {10.1002/hipo.22671},
	journal = {Hippocampus},
	journal-full = {Hippocampus},
	keywords = {MRI; harmonization; hippocampus; parahippocampal gyrus; segmentation},
	mesh = {Hippocampus; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Parahippocampal Gyrus; Pattern Recognition, Automated},
	month = {01},
