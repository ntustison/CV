	keywords = {Advanced Normalization Tools; convolutional neural network; deep learning; functional lung imaging; segmentation},
	mesh = {Algorithms; Ecosystem; Humans; Image Processing, Computer-Assisted; Lung; Magnetic Resonance Imaging; Retrospective Studies; Semantics; Xenon Isotopes},
	month = {11},
