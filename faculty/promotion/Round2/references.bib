	keywords = {anxiety; fMRI; generalization; hippocampus; mPFC; pattern completion},
	mesh = {Adolescent; Amygdala; Anxiety; Anxiety Disorders; Humans; Magnetic Resonance Imaging; Memory; Prefrontal Cortex},
	month = {Feb},
	number = {2},
