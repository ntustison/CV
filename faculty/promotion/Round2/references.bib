	journal = {Pediatr Pulmonol},
	journal-full = {Pediatric pulmonology},
	keywords = {helium-3 lung MRI; hyperpolarized lung MRI; maldistribution of ventilation; severe asthma; ventilation heterogeneity},
	mesh = {Asthma; Helium; Humans; Isotopes; Lung; Magnetic Resonance Imaging; Phenotype},
	month = {Jun},
