	doi = {10.1097/JCP.0000000000001120},
	journal = {J Clin Psychopharmacol},
	journal-full = {Journal of clinical psychopharmacology},
	mesh = {Adult; Aged; CA3 Region, Hippocampal; Clinical Trials as Topic; Dentate Gyrus; Female; Glucocorticoids; Humans; Magnetic Resonance Imaging; Male; Middle Aged; Neuroimaging; Prednisone; Young Adult},
	number = {6},
	pages = {653-657},
	pmc = {PMC6856429},
	pmid = {31688386},
	pst = {ppublish},
	title = {The Relationship Between Cumulative Exogenous Corticosteroid Exposure and Volumes of Hippocampal Subfields and Surrounding Structures},
	volume = {39},
