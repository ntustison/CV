	keywords = {3D-SSP; Asymmetry; Drug-resistant epilepsy; Imaging features; Machine learning; Open source; PET; Surgical outcome},
	mesh = {Drug Resistant Epilepsy; Electroencephalography; Epilepsy, Temporal Lobe; Fluorodeoxyglucose F18; Humans; Magnetic Resonance Imaging; Positron-Emission Tomography; Retrospective Studies; Seizures; Treatment Outcome},
	month = {03},
