	keywords = {MRI; hyperpolarized Xenon-129; idiopathic pulmonary fibrosis; interstitial lung disease},
	mesh = {Humans; Idiopathic Pulmonary Fibrosis; Lung; Magnetic Resonance Imaging; Xenon Isotopes},
