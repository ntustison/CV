	doi = {10.1016/j.jalz.2011.09.172},
	journal = {Alzheimers Dement},
	journal-full = {Alzheimer's \& dementia : the journal of the Alzheimer's Association},
	mesh = {Alzheimer Disease; Biomarkers; Early Diagnosis; Humans; Longitudinal Studies; Neuroimaging},
